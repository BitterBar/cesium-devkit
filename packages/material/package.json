{
  "name": "@cesium-devkit/material",
  "version": "0.0.1-beta.0",
  "description": "@cesium-devkit/material",
  "main": "index.js",
  "module": "dist/material.es.js",
  "types": "dist/material.d.ts",
  "files": [
    "index.js",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/material.d.ts",
      "node": {
        "production": "./dist/material.cjs.prod.js",
        "development": "./dist/material.cjs.js",
        "default": "./index.js"
      },
      "module": "./dist/material.es.js",
      "import": "./dist/material.es.js",
      "require": "./index.js"
    },
    "./*": "./*"
  },
  "buildOptions": {
    "name": "CesiumDevkitMaterials",
    "formats": [
      "es",
      "cjs",
      "umd"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BitterBar/cesium-devkit",
    "directory": "packages/material"
  },
  "keywords": [
    "cesium-devkit"
  ],
  "author": "Bitter Bar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BitterBar/cesium-devkit/issues"
  },
  "homepage": "https://github.com/BitterBar/cesium-devkit/tree/main/packages/material#readme",
  "devDependencies": {
    "cesium": "^1.120.0",
    "@cesium-devkit/shader": "workspace:*",
    "@cesium-devkit/shared": "workspace:*"
  },
  "peerDependencies": {
    "cesium": "^1.120"
  }
}
